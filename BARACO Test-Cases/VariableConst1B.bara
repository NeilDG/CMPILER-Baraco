public class VariableConst1B:
	void main():
		final int NOT_FOUND = 404;
		final int BAD_REQUEST = 400;
		final int REQUEST_TIMEOUT = 408;

		NOT_FOUND = BAD_REQUEST; //error #1
		BAD_REQUEST = 100; //error #2
		BAD_REQUEST++; //error #3
	
		println("HTTP error code of NOT_FOUND: " +NOT_FOUND);
		println("HTTP error code of BAD_REQUEST: " +BAD_REQUEST);
		println("HTTP error code of REQUEST_TIMEOUT: " +REQUEST_TIMEOUT);
	end
end
